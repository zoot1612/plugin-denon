<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
  <specVersion>
    <major>1</major>
    <minor>0</minor>
  </specVersion>
  <serviceStateTable>

    <stateVariable sendEvents="yes">
      <name>Input</name>
      <datatype>string</datatype>
    </stateVariable>

    <stateVariable sendEvents="yes">
      <name>Source</name>
      <datatype>string</datatype>
    </stateVariable>

    <stateVariable sendEvents="yes">
      <name>Surround</name>
      <datatype>string</datatype>
    </stateVariable>

    <stateVariable sendEvents="yes">
      <name>Command</name>
      <datatype>string</datatype>
    </stateVariable>

    <stateVariable sendEvents="yes">
      <name>PowerStatus</name>
      <datatype>boolean</datatype>
            <defaultValue>0</defaultValue>
            <shortCode>powerstatus</shortCode>
    </stateVariable>

  </serviceStateTable>
  
  <actionList>

    <action>
      <name>SetInputTarget</name>
      <argumentList>
        <argument>
          <name>newInputValue</name>
          <direction>in</direction>
                    <relatedStateVariable>Input</relatedStateVariable>
        </argument>
      </argumentList>
    </action>

    <action>
      <name>SendCommand</name>
      <argumentList>
        <argument>
          <name>Command</name>
          <direction>in</direction>
                    <relatedStateVariable>Command</relatedStateVariable>
        </argument>
      </argumentList>
    </action>

    <action>
      <name>Input11</name>
    </action>
    <action>
      <name>Input12</name>
    </action>
    <action>
      <name>Input13</name>
    </action>
    <action>
      <name>Input14</name>
    </action>
    <action>
      <name>Input15</name>
    </action>
    <action>
      <name>Input16</name>
    </action>
    <action>
      <name>Input17</name>
    </action>
    <action>
      <name>Input18</name>
    </action>
    <action>
      <name>Input19</name>
    </action>
    <action>
      <name>Input20</name>
    </action>
    <action>
      <name>Source</name>
    </action>



  </actionList>
</scpd>
